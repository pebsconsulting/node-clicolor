{
  "name": "clicolor",
  "version": "1.1.0",
  "description": "simple functions for using pretty colors in your CLI app",
  "keywords": [ "cli" ],
  "author": "Robey Pointer <robeypointer@gmail.com>",
  "homepage": "https://github.com/robey/node-clicolor",
  "repository": {
    "type": "git",
    "url": "git://github.com/robey/node-clicolor.git"
  },
  "license": "Apache-2.0",
  "dependencies": {
    "antsy": "*"
  },
  "devDependencies": {
    "babel": "latest",
    "mocha": "*",
    "should": "*",
    "source-map-support": "*"
  },
  "main": "./lib/clicolor",
  "files": [
    "lib"
  ],
  "scripts": {
    "build": "npm run prepublish",
    "clean": "rm -rf lib test/lib",
    "distclean": "npm run clean && rm -rf node_modules",
    "prepublish": "babel --source-maps --out-dir lib src && babel --source-maps --out-dir test/lib test/src",
    "test": "mocha -R spec --colors test/lib"
  }
}
